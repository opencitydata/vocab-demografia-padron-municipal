###  http://vocab.ciudadesabiertas.es/def/demografia/padron-municipal#nivelEstudios
:nivelEstudios rdf:type owl:ObjectProperty ;
               rdfs:domain :Habitante ;
               rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                   owl:hasValue <http://vocab.linkeddata.es/datosabiertos/kos/demografia/padron-municipal/tipo-nivel-estudio>
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "El nivel de estudios del habitante, que puede ser: sin estudios, bachiller elemental, bachiller superior, etc. Estos niveles de estudio se representan en la lista de conceptos: http://vocab.linkeddata.es/datosabieros/kos/demografia/padron-municipal/tipo-nivel-estudio"@es ,
                            "The educational level of an inhabitant, which can be: without studies, elementary bachelor, high school, etc. These levels are represented by the concept list: http://vocab.linkeddata.es/datosabieros/kos/demografia/padron-municipal/tipo-nivel-estudio"@en ;
               rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/demografia/padron-municipal> ;
               rdfs:label "Educational level"@en ,
                          "Nivel de estudios"@es .


